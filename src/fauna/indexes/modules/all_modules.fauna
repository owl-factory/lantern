// Creates an index for listing all modules. This is a temporary module index and should be tightened eventually
CreateIndex({
  name: "all_modules",
  source: Collection("modules"),
  values: [
    { field: ["data", "updatedAt"]},
    { field: ["ref"]},
    { field: ["data", "name"]},
    { field: ["data", "ruleset", "ref"]},
    { field: ["data", "ruleset", "name"]}
  ],
})