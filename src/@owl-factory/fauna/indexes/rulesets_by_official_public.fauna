// Creates an index for listing either officially supported rulesets or unofficial rulesets
CreateIndex({
  name: "rulesets_by_official_public",
  source: Collection("rulesets"),
  terms: [{ field: ["data", "isOfficial"]}, { field: ["data", "isPublic"]} ],
  values: [
    { field: ["data", "updatedAt"]},
    { field: ["ref"]},
    { field: ["data", "name"]},
    { field: ["data", "ownedBy"]},
    { field: ["data", "isPublic"]},
    { field: ["data", "isLocked"]},
  ],
})